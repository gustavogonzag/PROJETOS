
public class Main {

	public static void main(String[] args) {
		Conta conta = new Conta();
		
		conta.setNome("Paulo Ricardo Santos Nascimento");
		conta.setNumero(58546);
		conta.inicializa("Paulo Ricardo Santos Nascimento", 1000);
		conta.deposita(300);
		conta.consulta();
		conta.saque(200);
		conta.consulta();

	}

}
